{
  "name": "sql-playground",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --verbose --runInBand",
    "test:watch": "npm run test -- --watch",
    "start": "node server.js",
    "start:watch": "nodemon server.js",
    "lint": "eslint .",
    "create-tables": "node data/create-tables.js",
    "drop-tables": "node data/drop-tables.js",
    "load-seed-data": "node data/load-seed-data.js",
    "recreate-tables": "npm run drop-tables && npm run create-tables",
    "setup-db": "npm run recreate-tables && npm run load-seed-data",
    "setup-db:prod": "heroku run npm run setup-db",
    "setup-heroku": "heroku addons:create heroku-postgresql:hobby-dev && heroku config:set PGSSLMODE=require && heroku config:get DATABASE_URL"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jpendy/sql-playground.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jpendy/sql-playground/issues"
  },
  "homepage": "https://github.com/Jpendy/sql-playground#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.6",
    "pg": "^8.4.2"
  }
}
